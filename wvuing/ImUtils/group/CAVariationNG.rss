//  1: "show blocked list" option is shown as specified.
//
// By default this is enabled (1).
// ----------------------------------------------------
//
RESOURCE NUMBER_INT32 RSC_CRRSS_CHAT_VARIATION_SHOW_BLOCKED_LIST { value = 1; }
// RSC_CRRSS_CHAT_VARIATION_RECORD_WARNING
