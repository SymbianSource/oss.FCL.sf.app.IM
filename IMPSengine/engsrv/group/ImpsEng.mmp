// data
SOURCE impsdigestbytes.cpp
SOURCE impssdatautils.cpp
SOURCE impsorphans.cpp
SOURCE impsapplauncherproxy.cpp

// *************************************
// INCLUDE PATHS
// *************************************
USERINCLUDE ../inc ../../inc ../../datautils/inc ../../enginc

SYSTEMINCLUDE   ../../../inc
APP_LAYER_SYSTEMINCLUDE

